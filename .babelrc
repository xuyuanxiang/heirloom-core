{
    "ignore": "node_modules",
    "presets": [
        "react",
        "es2015",
        "stage-1"
    ],
    "plugins": [
        "transform-runtime",
        "transform-flow-strip-types"
    ],
    "env": {
        "commonjs": {
            "plugins": [
                [
                    "transform-es2015-modules-commonjs",
                    {
                        "loose": true
                    }
                ]
            ]
        }
    }
}
